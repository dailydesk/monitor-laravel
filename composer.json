{
    "name": "dailydesk/monitor-laravel",
    "description": "DailyDesk Monitor for Laravel applications.",
    "keywords": [
        "dailydesk",
        "monitor",
        "monitor-laravel"
    ],
    "homepage": "https://github.com/dailydesk/monitor-laravel",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "dailydesk/monitor-php": "dev-main",
        "illuminate/console": "^11",
        "illuminate/contracts": "^11",
        "illuminate/http": "^11",
        "illuminate/support": "^11"
    },
    "require-dev": {
        "inspector-apm/inspector-laravel": "^4.9",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "DailyDesk\\Monitor\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\DailyDesk\\Monitor\\Laravel\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DailyDesk\\Monitor\\Laravel\\MonitorServiceProvider"
            ],
            "aliases": {
                "Monitor": "DailyDesk\\Laravel\\Facades\\Monitor"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
